<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  {% block head %}
  <title>{{ title is defined ? title ~ " | " }}Fast Response</title>

  <meta name="description" content="" />

  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <meta charset="utf-8" />
  <meta name="robots" content="INDEX, FOLLOW">
  <meta name="googlebot" content="INDEX, FOLLOW">

  <meta name="viewport" content="width=device-width,initial-scale=1" />

  <link type="image/x-icon" rel="shortcut icon" href="/favicon.ico" />

  <link type="text/css" rel="stylesheet" media="all" href="/css/reset.css" />
  <link type="text/css" rel="stylesheet" media="all" href="/css/base.css" />
  <link type="text/css" rel="stylesheet" media="all" href="/css/footer.css" />

  {% include 'analytics_google.html' %}

  {{ jquery is defined ? "<script type='text/javascript' src='/js/jquery-1.10.2.min.js'></script>" }}

  {{ scripts|default('')|raw }}

  {{ styles|default('')|raw }}

  {% endblock head %}
</head>

<body>
<div id="page">

  {% block top %}
  <header>
    <div class="clearfix"></div>
  </header>
  {% endblock top %}

  <aside class="left">
    {% block left %}{% endblock left %}
  </aside>

  <section id="main">
    {% block maincontent %}
    <main>
      {{ content|raw }}
    </main>
    {% endblock maincontent %}
    <div class="clearfix"></div>
  </section> <!-- /main -->

  <aside class="right">
    {% block right %}{% endblock right %}
  </aside>

  <footer id="footer">
    {% include 'footer.twig' %}
  </footer> <!-- /footer -->

</div> <!-- /page -->
</body>
</html>
